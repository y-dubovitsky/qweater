<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation=
               "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd
                http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="ironWindow" class="spel.component.impl.IronWindow"/>

    <context:property-placeholder location="classpath:spel/app.properties" />

    <bean id="sweetHome" class="spel.component.impl.SweetHome">
        <property name="window" value="#{T(space.dubovitsky.spring.spel.component.factory.WindowFactory).getWindow('WoodenWindow')}"/>
    </bean>


</beans>